
public class P37 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num=0; 
		num++;
		System.out.println("월요일 알의 개수 "+"= "+num);
		num++;
		System.out.println("화요일 알의 개수 "+"= "+num);
		num++;
		System.out.println("수요일 알의 개수 "+"= "+num);
		num++;
		System.out.println("목요일 알의 개수 "+"= "+num);
		num++;
		System.out.println("금요일 알의 개수 "+"= "+num);
		num++;
		System.out.println("토요일 알의 개수 "+"= "+num);
		num++;
		System.out.println("일요일 알의 개수 "+"= "+num);
		
	}

}
