package nyw;

public class gender {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int gender = 2;
		//만약 gender의 값이 2라면 
		{
		
			//"여자"를 출력해주세요
		
		if ( gender == 2);
			
			System.out.println("여자");
		}
		
		if ( gender < 3);
		{
			System.out.println("남자");
		}
		
		
	}

}
