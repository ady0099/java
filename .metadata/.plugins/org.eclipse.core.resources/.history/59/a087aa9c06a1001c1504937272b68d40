package csr;

public class Calor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int e = 20 >> 2;
		int b = 20 >>> 2;
		int c = 20 << 2;
		int a = -20 >>> 2;
		int d = -20 >> 2;
		
		System.out.println(e);
		System.out.println(b);
		System.out.println(c); 
		System.out.println(a);
		System.out.println(d);
		
		
	}

}
